  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="CountryId">
          <DataField>CountryId</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="CountryName">
          <DataField>CountryName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TotalSales">
          <DataField>TotalSales</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT     DimCountry.CountryId, DimCountry.CountryName, SUM(FactCustomer.CustomerAmount) AS TotalSales
FROM         DimCountry INNER JOIN
                      FactCustomer ON DimCountry.CountryId = FactCustomer.Countryid_fk
GROUP BY DimCountry.CountryId, DimCountry.CountryName
HAVING      (DimCountry.CountryId = @CountryId)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@CountryId">
            <Value>=Parameters!CountryId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
