  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Cid">
            <Value>=Parameters!Cid.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     FactCustomer.CustomerName, FactCustomer.CustomerAmount, FactCustomer.SalesDate, DimCountry.CountryName, DimProduct.ProductName, 
                      DimSalesPerson.SalesPersonName, DimStates.StatesName
FROM         FactCustomer INNER JOIN
                      DimSalesPerson ON FactCustomer.SalesPersonId_fk = DimSalesPerson.SalesPersonId INNER JOIN
                      DimStates ON FactCustomer.StatesId_fk = DimStates.StatesId INNER JOIN
                      DimCountry ON FactCustomer.Countryid_fk = DimCountry.CountryId INNER JOIN
                      DimProduct ON FactCustomer.Productid_fk = DimProduct.ProductId
WHERE     (FactCustomer.Countryid_fk = @Cid)</CommandText>
      </Query>
