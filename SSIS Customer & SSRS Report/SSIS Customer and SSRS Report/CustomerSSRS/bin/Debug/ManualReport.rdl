  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT     FactCustomer.CustomerCode, FactCustomer.CustomerName, FactCustomer.CustomerAmount, FactCustomer.SalesDate, DimStates.StatesName, 
                      DimSalesPerson.SalesPersonName, DimProduct.ProductName, DimCountry.CountryName
FROM         DimCountry INNER JOIN
                      FactCustomer INNER JOIN
                      DimSalesPerson ON FactCustomer.SalesPersonId_fk = DimSalesPerson.SalesPersonId INNER JOIN
                      DimStates ON FactCustomer.StatesId_fk = DimStates.StatesId INNER JOIN
                      DimProduct ON FactCustomer.Productid_fk = DimProduct.ProductId ON DimCountry.CountryId = FactCustomer.Countryid_fk</CommandText>
      </Query>
